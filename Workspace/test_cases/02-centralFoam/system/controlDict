libs (  "libfluidThermophysicalModels.so" "libmyFoam.so" "libvelocityOutletNSCBCx.so" "libvelocityOutletNSCBCy.so" "libvelocityOutletNSCBCz.so"  "libwaveTransmision.so" "libpressureOutletNSCBC.so" "libtemperatureOutletNSCBC.so");
application     rhoCentralFoam;

startFrom    startTime;

startTime    0;

stopAt    endTime;

endTime    0.05;

deltaT     1.0e-9;

writeControl  adjustableRunTime;

writeInterval    1.0e-6;

purgeWrite    0;

writeFormat    binary;

writePrecision   8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable    true;

adjustTimeStep  yes;

maxCo           0.25;

maxDeltaT       1e-5;

