    inlet
    {
        type            calculated;
        value           uniform 1.46507;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    bottom
    {
        type            symmetryPlane;
    }
    top
    {
        type            symmetryPlane;
    }
    obstacle1
    {
        type            calculated;
        value           uniform 0;
    }
    obstacle2
    {
        type            calculated;
        value           uniform 0;
